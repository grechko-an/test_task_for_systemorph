{
  "name": "protractor-ts-test-task",
  "version": "1.0.0",
  "description": "Runs Protractor E2E tests from a Typescript setup",
  "author": {
    "name": "Alex Grechko",
    "email": "4iginitra83@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "webdriver-update": "node ./node_modules/protractor/bin/webdriver-manager update",
    "pretest": "npm run build && npm run webdriver-update",
    "test": "protractor ./dist/conf.js",
    "predebug": "npm run build",
    "debug": "protractor ./dist/conf.js"
  },
  "dependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.0.2",
    "@types/chai": "^4.1.7",
    "jasmine": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "protractor": "^5.4.2",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-smoothie": "^0.3.2",
    "ts-node": "^8.1.0"
  }
}
